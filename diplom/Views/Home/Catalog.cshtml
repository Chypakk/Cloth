@model ProductsListViewModel

@{
    Layout = "_LayoutWithCategories";
}


@foreach (var p in Model.Products)
{
    @Html.Partial("_Catalog", p);
}
<div page-model="@Model.PagingInfo" page-action="Catalog" page-classes-enabled="true"
     page-class="btn" page-class-normal="btn-secondary"
     page-class-selected="btn-primary" page-url-category="@Model.CurrentCategory"
     class="btn-group pull-right m-1"></div>

